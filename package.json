{
  "name": "singelong",
  "displayName": "SingeLong",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "1.78.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "custom-activitybar",
          "title": "SingeLong",
          "icon": "assets/logo_bito.svg"
        }
      ]
    },
    "views": {
      "custom-activitybar": [
        {
          "type": "webview",
          "id": "singelong.openview",
          "name": "Lyrics",
          "contextualTitle": "SingeLong"
        }
      ]
    },
    "commands": [
      {
        "command": "singelong.accessToken",
        "title": "Get Access Token",
        "category": "SingeLong"
      },
      {
        "command": "singelong.logout",
        "title": "Logout",
        "category": "SingeLong"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "1.78.1",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "axios": "^1.7.7",
    "express": "^4.19.2"
  }
}
